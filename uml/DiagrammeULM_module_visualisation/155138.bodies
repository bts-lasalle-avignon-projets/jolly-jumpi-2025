    if(sockets.contains(socket->peerAddress().toString()))
    {
        // on nettoie
        delete sockets[socket->peerAddress().toString()];
        sockets.remove(socket->peerAddress().toString());
        peripheriques.remove(socket->peerAddress().toString());
    }

