    if(!GESTION_APPAIRAGE)
        return;
    if(peripherique.name().startsWith(PREFIX_NOM_MOD_CONFIGURATION) ||
       peripherique.name().startsWith(PREFIX_NOM_PISTE))
    {
        QBluetoothLocalDevice::Pairing etatAppairage =
