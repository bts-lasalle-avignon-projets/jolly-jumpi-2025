            break;
        case QBluetoothSocket::OperationError:
            messageErreur = "Erreur d'opération Bluetooth";
            break;
        default:
            messageErreur = QString("Erreur inconnue (code %1)")
                              .arg(static_cast<int>(erreur));
            break;
    }

